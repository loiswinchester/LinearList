#include<stdlib.h>
#define InitSize 10//顺序表的初始长度，元素个数
typedef struct {
	int *data;//指示动态分配数组的指针，指向第一个数据元素
	int Maxsize;//1顺序表最大容量
	int length;
}SqList;

void InitList(SqList L) {
	L.data = (int*)malloc(InitSize * sizeof(int));//申请一片连续的存储空间，返回指针，需要强制转换
	L.length = 0;
	L.Maxsize = InitSize;

}

void IncresaeSize(SqList L, int len) {//c没有引用&，&是c++的
	int* p = L.data;
	int len = 0;
		L.data = (int*)malloc((InitSize + len) * sizeof(int));
	for (int i = 0; i < L.length; i++)
	{
		L.data[i] = p[i];
	}
	L.Maxsize = L.length + len;
	free(p);
}

int main() {
	SqList L;
	InitList(L);
	IncresaeSize(L, 5);
	return 0;
}
